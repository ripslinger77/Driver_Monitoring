import requests
import json

# Login to MEGA and get the session ID and master key
data = {'user': '1ms19et033@gmail.com', 'password': 'Chakee@19'}
response = requests.post('https://g.api.mega.co.nz/v1/user/login', json=data)
result = json.loads(response.text)

# Get the folder ID for the existing folder
folder_name = 'TEst'
data = {'a': 'f', 'c': 1, 'r': 1}
response = requests.post('https://g.api.mega.co.nz/v1/fs', params=data, headers={'sid': result['t']})
result = json.loads(response.text)

folder_id = None
for folder in result['f']:
    if folder['t'] == folder_name:
        folder_id = folder['h']
        break

if folder_id is None:
    print('Folder not found')
    exit(1)

# Upload the file to the folder
file_path = 'C:/D Drive Contents/Driver assist - Internship/Driver_monitoring/Repos'
with open(file_path, 'rb') as file:
    data = {'a': 'p', 't': folder_id, 'n': file}
    response = requests.post('https://g.api.mega.co.nz/v1/upload', params=data, headers={'sid': result['t']})
    result = json.loads(response.text)

print('File uploaded with ID', result['p'])